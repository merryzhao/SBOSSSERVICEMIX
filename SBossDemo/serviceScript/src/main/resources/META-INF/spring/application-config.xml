<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- Uncomment and add your base-package here: -->
	<context:component-scan
		base-package="com.ai.sboss.serviceScript,
            com.ai.sboss.serviceScript.processor.*,
            com.ai.sboss.serviceScript.respconvertor.*,
            com.ai.sboss.serviceScript.parameter.*,
            com.ai.sboss.serviceScript.urlRequest.*" />

	<bean id="serviceScriptParameter"
		class="com.ai.sboss.serviceScript.parameter.ServiceScriptParameter" />
	<bean id="orderScriptParameter"
		class="com.ai.sboss.serviceScript.parameter.OrderScriptParameter" />
	<bean id="processScriptParameter"
		class="com.ai.sboss.serviceScript.parameter.ProcessScriptParameter" />

	<bean id="httpRequest" class="com.ai.sboss.serviceScript.urlRequest.HttpRequest" />

</beans>
