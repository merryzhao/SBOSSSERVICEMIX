		<camel:route id = "findAnalyseResult">
			<!-- 前端调用findAnalyseResult接口，启动如下的route -->
			<camel:from uri="jetty:http://0.0.0.0:8282/sboss/findAnalyseResult" />
			<camel:process ref="generalJettyInProcessor" />
			<!-- 获取从前端传入的content_id列表 -->
			<camel:process ref = "getContentIdsInProcessor" />
			<!-- 遍历传入的content_id列表，调用crm服务获得该content_id在文件服务器对应的url地址 -->
			<camel:split strategyRef = "contentsInfoAggregation">
				<camel:tokenize token=";" />
				<camel:to uri="direct:queryContentById" />
				<!-- 获取到url地址之后，从文件服务器取出content_id对应的文本信息 -->
				<camel:process ref="querySingleContentOutProcessor" />	
			</camel:split>
			<!-- 将返回的json数据包装成前端要求的返回参数格式 -->
			<camel:process ref="findAnalyseResultOutProcessor"/>
			<camel:process ref="getFinalAnalyseResultOutProcessor" />
		</camel:route>

	</camelContext>

</blueprint>
